// neutral thrash 2.slim : ^Total mutation frequencies: (.*)$
//
//
initialize() {
	initializeMutationRate(0.2);
	initializeMutationType("m1", 0.5, "f", 0.0);
	initializeGenomicElementType("g1", m1, 1.0);
	initializeGenomicElement(g1, 0, 9);
	initializeRecombinationRate(0.2);
}
1 early() {
	sim.addSubpop("p1", 4);
}
2000 late() { cat("\nTotal mutation frequencies: " + sum(sim.mutationFrequencies(NULL)) + "\n"); }
